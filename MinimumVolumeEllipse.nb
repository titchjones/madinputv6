       RowBox[{"PseudoInverse", "[", 
        RowBox[{
         RowBox[{"P", ".", "U", ".", 
          RowBox[{"Transpose", "[", "P", "]"}]}], " ", "-", " ", 
         RowBox[{
          RowBox[{"Transpose", "[", 
           RowBox[{"{", 
            RowBox[{"P", ".", "u"}], "}"}], "]"}], ".", 
          RowBox[{"{", 
           RowBox[{"P", ".", "u"}], "}"}]}]}], "]"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"c", "=", 
      RowBox[{"P", ".", "u"}]}], ";", "\[IndentingNewLine]", 
