Cell[BoxData[
 RowBox[{
  RowBox[{"khachiyan", "[", "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"X", "=", 
      RowBox[{"Q", ".", 
       RowBox[{"DiagonalMatrix", "[", "u", "]"}], ".", 
       RowBox[{"Transpose", "[", "Q", "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"M", "=", 
      RowBox[{"Diagonal", "[", 
       RowBox[{
        RowBox[{"Transpose", "[", "Q", "]"}], ".", 
        RowBox[{"PseudoInverse", "[", "X", "]"}], ".", "Q"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"maximum", "=", 
      RowBox[{"Max", "[", "M", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"j", "=", 
      RowBox[{
       RowBox[{"Position", "[", 
        RowBox[{"M", ",", "maximum"}], "]"}], "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "1"}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"stepsize", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"maximum", "-", "d", "-", "1"}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"d", "+", "1"}], ")"}], 
         RowBox[{"(", 
          RowBox[{"maximum", "-", "1"}], ")"}]}], ")"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"newu", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "-", "stepsize"}], ")"}], "u"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"newu", "[", 
       RowBox[{"[", "j", "]"}], "]"}], "=", 
      RowBox[{
       RowBox[{"newu", "[", 
        RowBox[{"[", "j", "]"}], "]"}], "+", "stepsize"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"count", "=", 
      RowBox[{"count", "+", "1"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"err", "=", 
      RowBox[{"Norm", "[", 
       RowBox[{"newu", "-", "u"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"u", "=", "newu"}], ";", "\[IndentingNewLine]", "err"}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
