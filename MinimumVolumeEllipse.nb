Cell[BoxData[
 RowBox[{
  RowBox[{"MinimumVolumeEllipse", "[", 
   RowBox[{"P_", ",", "tol_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "d", ",", "Np", ",", "Q", ",", "count", ",", "err", ",", "u", ",", "U", 
      ",", "A", ",", "c"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"d", ",", "Np"}], "}"}], "=", 
      RowBox[{"Dimensions", "[", "P", "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"Q", "=", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"0", ",", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"d", "+", "1"}], ",", "Np"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Q", "[", 
       RowBox[{"[", 
        RowBox[{"1", ";;", "d"}], "]"}], "]"}], "=", 
      RowBox[{"P", "[", 
       RowBox[{"[", 
        RowBox[{
         RowBox[{"1", ";;", "d"}], ",", 
         RowBox[{"1", ";;", "Np"}]}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"Q", "[", 
       RowBox[{"[", 
        RowBox[{"d", "+", "1"}], "]"}], "]"}], "=", 
      RowBox[{"ConstantArray", "[", 
       RowBox[{"1", ",", 
        RowBox[{"{", "Np", "}"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"count", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"err", "=", "1"}], ";", "\[IndentingNewLine]", 
     RowBox[{"u", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "/", "Np"}], ")"}], "*", 
       RowBox[{"ConstantArray", "[", 
        RowBox[{"1", ",", 
         RowBox[{"{", "Np", "}"}]}], "]"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
