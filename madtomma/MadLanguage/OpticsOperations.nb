Cell["\<\
Need to add pattern matching test that seq contains right things.  \
\>", "Text"],
Cell[BoxData[
    \(\(elementIntervals[seq_mfs] := 
      N[Transpose[{mfsColumn[seq, "\<NAME\>"], 
            mfsColumn[seq, "\<S\>"] - \((mfsColumn[seq, "\<L\>"])\)/2, 
            mfsColumn[seq, "\<S\>"] + \((mfsColumn[seq, "\<L\>"])\)/2}], 10]; 
    \)\)], "Input",
  InitializationCell->True],
Cell[BoxData[
    RowBox[{
      RowBox[{"findElement", "[", 
        RowBox[{"seqints_List", ",", 
          RowBox[{"s_", "/;", 
            RowBox[{"NumericQ", "[", "s", "]"}]}]}], "]"}], ":=", 
      RowBox[{"Select", "[", 
        RowBox[{"seqints", ",", 
          RowBox[{
            RowBox[{"(", 
              RowBox[{
                RowBox[{
                  RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}], "<", "s"}], " ", "&&", 
                " ", 
                RowBox[{"s", "<", 
                  RowBox[{"#", "[", 
                    RowBox[{"[", "3", "]"}], "]"}]}]}], ")"}], "&"}]}], 
        "]"}]}]], "Input",
  InitializationCell->True],
Cell[BoxData[
    \(findElement[seq_mfs, s_ /; NumericQ[s]] := 
      findElement[elementIntervals[seq], s]\)], "Input",
  InitializationCell->True],
Cell[BoxData[
    \(findElement[seq_mfs, ss_List] := 
      Module[{seqints}, \n\t\tseqints = elementIntervals[seq]; \n\t\t
        Map[findElement[seqints, #]&, ss]]\)], "Input",
  InitializationCell->True],
Cell[BoxData[
    RowBox[{
      RowBox[{"findElement", "[", 
        RowBox[{"seq_mfs", ",", "x_"}], "]"}], ":=", 
      RowBox[{"{", 
        RowBox[{"{", "}"}], "}"}]}]], "Input",
  InitializationCell->True],
Cell[BoxData[
    \(\(splitElement[seqname_String, {el_String, s1_Real, s2_Real}, 
        s_ /; NumericQ[s]] := 
      Module[{el1 = el<>"\<.1\>", el2 = el<>"\<.2\>", def1, def2}, \n
        \t\t{MADnewElement[el1, {el, "\<L\>" \[Rule] \((s - s1)\)}], \n\t\t\t
          MADnewElement[el2, {el, "\<L\>" \[Rule] \((s2 - s)\)}], \n\t\t\t
          MADseqedit[seqname, \n
            \t\t\t{MADinstall[el1, "\<AT\>" \[Rule] \((s + s1)\)\/2 - s, 
                "\<FROM\>" \[Rule] el], \n\t\t\t\t
              MADinstall[el2, "\<AT\>" \[Rule] \((s + s2)\)\/2 - s, 
                "\<FROM\>" \[Rule] el], \n\t\t\t\tMADremove[el]}\n\t\t]\n\t\t}
          \n\t]\n\t\t\t\)\)], "Input",
  InitializationCell->True],
Cell[BoxData[
    \(splitElement[seqname_String, seq_mfs, 
        s_List /; And@@\((NumericQ/@s)\)] := 
      \((\(splitElement[seqname, seq, #]&\)/@\ s)\)\)], "Input",
  InitializationCell->True],
Cell[BoxData[
    \(splitElement[seqname_String, seq_mfs, s_ /; NumericQ[s]] := 
      Module[{elint}, \n\t\telint = findElement[seq, s]; \n\t\t
        If[elint == {}, {}, splitElement[seqname, First[elint], s]]\n\t]\)], 
  "Input",
  InitializationCell->True],
Cell[BoxData[
    \(splitElement[_, {}, s_] := {}\)], "Input",
  InitializationCell->True],
Cell[BoxData[
    \(splitElement[seqname_String, seq_, x_] := 
      MADcomment["\<Invalid azimuth given to splitElement\>"]\)], "Input",
  InitializationCell->True]
