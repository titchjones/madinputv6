Cell["Definition of the exported functions", "Subsection",
  InitializationCell->True,
  CellTags->"Implementation section"],

Cell[BoxData[
    RowBox[{
      RowBox[{"SymplecticJ", "[", 
        RowBox[{"n_Integer", "/;", 
          RowBox[{"n", ">", "0"}]}], "]"}], ":=", 
      RowBox[{
        RowBox[{"SymplecticJ", "[", "n", "]"}], "=", 
        RowBox[{"Block", "[", 
          RowBox[{
            RowBox[{"{", "s", "}"}], ",", 
            RowBox[{
              RowBox[{"s", "=", 
                RowBox[{"Table", "[", 
                  RowBox[{"0", ",", 
                    RowBox[{"{", 
                      RowBox[{"2", " ", "n"}], "}"}], ",", 
                    RowBox[{"{", 
                      RowBox[{"2", " ", "n"}], "}"}]}], "]"}]}], ";", 
              RowBox[{"s", "=", 
                RowBox[{"ReplacePart", "[", 
                  RowBox[{"s", ",", "1", ",", 
                    RowBox[{"Table", "[", 
                      RowBox[{
                        RowBox[{"{", 
                          RowBox[{
                            RowBox[{
                              RowBox[{"2", " ", "k"}], "-", "1"}], ",", 
                            RowBox[{"2", " ", "k"}]}], "}"}], ",", 
                        RowBox[{"{", 
                          RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}]}],
                   "]"}]}], ";", 
              RowBox[{"ReplacePart", "[", 
                RowBox[{"s", ",", 
                  RowBox[{"-", "1"}], ",", 
                  RowBox[{"Table", "[", 
                    RowBox[{
                      RowBox[{"{", 
                        RowBox[{
                          RowBox[{"2", " ", "k"}], ",", 
                          RowBox[{
                            RowBox[{"2", " ", "k"}], "-", "1"}]}], "}"}], ",",
                       
                      RowBox[{"{", 
                        RowBox[{"k", ",", "1", ",", "n"}], "}"}]}], "]"}]}], 
                "]"}]}]}], "]"}]}]}]], "Input",
  InitializationCell->True],

Cell[BoxData[
    RowBox[{
      RowBox[{"evenSquareMatrixQ", "[", 
        RowBox[{"mat_", "?", "MatrixQ"}], "]"}], ":=", 
      RowBox[{
        RowBox[{"(", 
          RowBox[{
            RowBox[{
              RowBox[{"SameQ", "@@", "#1"}], "&&", 
              RowBox[{"EvenQ", "[", 
                RowBox[{"First", "[", "#1", "]"}], "]"}]}], "&"}], ")"}], "[",
         
        RowBox[{"Dimensions", "[", "mat", "]"}], "]"}]}]], "Input",
  InitializationCell->True],

Cell[BoxData[
    RowBox[{
      RowBox[{"SymplecticDimension", "[", 
        RowBox[{"mat_", "?", "evenSquareMatrixQ"}], "]"}], ":=", 
      RowBox[{
        FractionBox["1", "2"], " ", 
        RowBox[{"First", "[", 
          RowBox[{"Dimensions", "[", "mat", "]"}], "]"}]}]}]], "Input",
  InitializationCell->True],

Cell[BoxData[
    RowBox[{
      RowBox[{
        RowBox[{"Options", "[", "SymplecticQ", "]"}], "=", 
        RowBox[{"{", 
          RowBox[{"SymplecticTolerance", "\[Rule]", "0"}], "}"}]}], 
      ";"}]], "Input",
  InitializationCell->True],

Cell[BoxData[
    RowBox[{
      RowBox[{"SymplecticQ", "[", 
        RowBox[{
          RowBox[{"mat_", "?", "evenSquareMatrixQ"}], ",", "opts___"}], "]"}],
       ":=", 
      RowBox[{"Module", "[", 
        RowBox[{
          RowBox[{"{", 
            RowBox[{"j", "=", 
              RowBox[{"SymplecticJ", "[", 
                RowBox[{"SymplecticDimension", "[", "mat", "]"}], "]"}]}], 
            "}"}], ",", 
          RowBox[{"If", "[", 
            RowBox[{
              RowBox[{
                RowBox[{"Min", "[", 
                  RowBox[{"Precision", "/@", 
                    RowBox[{"Flatten", "[", "mat", "]"}]}], "]"}], "<", 
                "\[Infinity]"}], ",", 
              RowBox[{
                RowBox[{"MatrixNorm", "[", 
                  RowBox[{
                    RowBox[{
                      RowBox[{"Transpose", "[", "mat", "]"}], ".", "j", ".", 
                      "mat"}], "-", "j"}], "]"}], "\[LessEqual]", 
                RowBox[{"(", 
                  RowBox[{
                    RowBox[{
                    "SymplecticTolerance", "/.", "\[InvisibleSpace]", 
                      RowBox[{"{", "opts", "}"}]}], "/.", "\[InvisibleSpace]",
                     
                    RowBox[{"Options", "[", "SymplecticQ", "]"}]}], ")"}]}], 
              ",", 
              RowBox[{"Simplify", "[", 
                RowBox[{
                  RowBox[{
                    RowBox[{"Transpose", "[", "mat", "]"}], ".", "j", ".", 
                    "mat"}], "==", "j"}], "]"}]}], "]"}]}], "]"}]}]], "Input",\

  InitializationCell->True],

Cell[BoxData[
    RowBox[{
      RowBox[{"SymplecticQ", "[", 
        RowBox[{
          RowBox[{"mat_", "?", "MatrixQ"}], ",", "opts___"}], "]"}], ":=", 
      "False"}]], "Input",
  InitializationCell->True],

Cell[BoxData[
    RowBox[{
      RowBox[{"SymplecticConjugate", "[", 
        RowBox[{"mat_", "?", "MatrixQ"}], "]"}], ":=", 
      RowBox[{"Module", "[", 
        RowBox[{
          RowBox[{"{", 
            RowBox[{"j", "=", 
              RowBox[{"SymplecticJ", "[", 
                RowBox[{"SymplecticDimension", "[", "mat", "]"}], "]"}]}], 
            "}"}], ",", 
          RowBox[{"-", 
            RowBox[{"j", ".", 
              RowBox[{"Transpose", "[", "mat", "]"}], ".", "j"}]}]}], 
        "]"}]}]], "Input",
  InitializationCell->True],

Cell["Any other type of argument will not give an error message.", "Text"],

Cell[BoxData[
    RowBox[{
      RowBox[{"SymplecticInverse", "[", 
        RowBox[{"mat_", "?", "SymplecticQ"}], "]"}], ":=", 
      RowBox[{"SymplecticConjugate", "[", "mat", "]"}]}]], "Input",
  InitializationCell->True],

Cell[BoxData[
    RowBox[{
      RowBox[{"SymplecticInverse", "[", 
        RowBox[{"mat_", "?", "MatrixQ"}], "]"}], ":=", 
      RowBox[{"(", 
        RowBox[{
          RowBox[{"Message", "[", 
            RowBox[{"Symplectic", "::", "nonsymplectic"}], "]"}], ";", 
          RowBox[{"HoldForm", "[", 
            RowBox[{"SymplecticInverse", "[", "mat", "]"}], "]"}]}], 
        ")"}]}]], "Input",
  InitializationCell->True]
