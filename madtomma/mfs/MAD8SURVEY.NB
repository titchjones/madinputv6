 StyleBox["[]", "Input"]
}], "Subsubsection",
 InitializationCell->True,
 CellTags->"Implementation section"],

Cell["\<\
13/9/2000 discovered that SLAC MAD8acc adds an 2 extra columns beyond line 80 \
of the first line of each element record containing the beam energy.  Here we \
truncate all the element record lines at 80 columns in order to get rid of \
this and maintain compatibility with Unix  MAD8 in CERN.\
\>", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"surveyTapeRead", "[", "fn_String", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "str", ",", "ans", ",", "rawFile", ",", "header", ",", "trailer", ",", 
      RowBox[{"colheads", "=", 
       RowBox[{"{", 
        RowBox[{
        "\"\<KEYWORD\>\"", ",", "\"\<NAME\>\"", ",", "\"\<L\>\"", ",", 
         "\"\<ANGLE\>\"", ",", "\"\<K1\>\"", ",", "\"\<K2\>\"", ",", 
         "\"\<TILT\>\"", ",", "\"\<E1\>\"", ",", "\"\<E2\>\"", ",", 
         "\"\<H1\>\"", ",", "\"\<H2\>\"", ",", "\"\<X\>\"", ",", "\"\<Y\>\"", 
         ",", "\"\<Z\>\"", ",", "\"\<SUML\>\"", ",", "\"\<THETA\>\"", ",", 
         "\"\<PHI\>\"", ",", "\"\<PSI\>\""}], "}"}]}]}], 
     "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"rawFile", "=", 
      RowBox[{"ReadList", "[", 
       RowBox[{"fn", ",", "Record"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"rawFile", "===", "$Failed"}], ",", "rawFile", ",", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"header", "=", 
         RowBox[{"Take", "[", 
          RowBox[{"rawFile", ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"rawFile", "=", 
         RowBox[{"Drop", "[", 
          RowBox[{"rawFile", ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"rawFile", "=", 
         RowBox[{
          RowBox[{
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"StringLength", "[", "#", "]"}], ">", "80"}], ",", 
             RowBox[{"StringTake", "[", 
              RowBox[{"#", ",", "80"}], "]"}], ",", "#"}], "]"}], "&"}], 
          "\[IndentingNewLine]", "/@", "rawFile"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"rawFile", "=", 
         RowBox[{"StringJoin", "/@", 
          RowBox[{"Partition", "[", 
           RowBox[{"rawFile", ",", "4"}], "]"}]}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"rawFile", "=", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{"Flatten", "[", 
             RowBox[{"{", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"StringTake", "[", 
                RowBox[{"#1", ",", "4"}], "]"}], ",", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{"ans", "=", 
                 RowBox[{"ReadList", "[", 
                  RowBox[{
                   RowBox[{"str", "=", 
                    RowBox[{"StringToStream", "[", 
                    RowBox[{"StringDrop", "[", 
                    RowBox[{"#1", ",", "4"}], "]"}], "]"}]}], ",", 
                   RowBox[{"{", 
                    RowBox[{"Word", ",", 
                    RowBox[{"Table", "[", 
                    RowBox[{"Number", ",", 
                    RowBox[{"{", "16", "}"}]}], "]"}]}], "}"}]}], "]"}]}], 
                ";", 
                RowBox[{"Close", "[", "str", "]"}], ";", "ans"}]}], "}"}], 
             "\[IndentingNewLine]", "]"}], "&"}], ")"}], "/@", "rawFile"}]}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"mfs", "[", 
         RowBox[{
          RowBox[{"{", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{"\"\<HEADER\>\"", ",", "header"}], "}"}], ",", 
            "\[IndentingNewLine]", 
            RowBox[{"{", 
             RowBox[{
             "\"\<COMMENT\>\"", ",", 
              "\"\<mfs data object created from MAD Version 8 tape file \
format.\>\""}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"\"\<FILE\>\"", ",", "fn"}], "}"}]}], 
           "\[IndentingNewLine]", "}"}], ",", "\[IndentingNewLine]", 
          "colheads", ",", "\[IndentingNewLine]", "rawFile"}], 
         "\[IndentingNewLine]", "]"}]}]}], "\[IndentingNewLine]", "]"}]}]}], 
   "\[IndentingNewLine]", "]"}]}]], "Input",
 InitializationCell->True,
 CellChangeTimes->{{3.4476673502893744`*^9, 3.447667369992625*^9}}]
}, Open  ]]
}, Open  ]],

