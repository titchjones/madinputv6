Cell[CellGroupData[{

Cell["colorListPlot ", "Subsubsection",
 CellDingbat->None],

Cell["\<\
The main definition expects to receive a list of pairs of coordinates.\
\>", "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"colorListPlot", "[", 
   RowBox[{
    RowBox[{"x", ":", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"_", ",", " ", "_"}], "}"}], ".."}], "}"}]}], ",", 
    "opts___Rule"}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"ptdots", ",", "joins", ",", "pcolr"}], "}"}], ",", "\n", "\t\t", 
    RowBox[{
     RowBox[{"joins", "=", 
      RowBox[{
       RowBox[{"PlotJoined", "/.", 
        RowBox[{"{", "opts", "}"}]}], "/.", 
       RowBox[{"Options", "[", "colorListPlot", "]"}]}]}], ";", "\n", "\t\t", 
     
     RowBox[{"pcolr", " ", "=", " ", 
      RowBox[{
       RowBox[{"ColorScale", "/.", 
        RowBox[{"{", "opts", "}"}]}], "/.", 
       RowBox[{"Options", "[", "colorListPlot", "]"}]}]}], ";", "\n", "\t\t", 
     
     RowBox[{"If", "[", 
      RowBox[{"joins", ",", "\n", "\t\t\t", 
       RowBox[{"(", "\t", 
        RowBox[{
         RowBox[{"ptdots", "=", 
          RowBox[{"MapIndexed", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"Flatten", "[", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"pcolr", "[", 
                  RowBox[{
                   RowBox[{"First", "[", "#2", "]"}], "/", 
                   RowBox[{"Length", "[", "x", "]"}]}], "]"}], ",", 
                 RowBox[{"Line", "[", "#1", "]"}]}], "}"}], "]"}], ")"}], 
             "&"}], ",", 
            RowBox[{"Partition", "[", 
             RowBox[{"x", ",", "2", ",", "1"}], "]"}]}], "]"}]}], ";"}], 
        ")"}], ",", "\n", "\t\t\t", 
       RowBox[{"(", "\t", 
        RowBox[{
         RowBox[{"ptdots", "=", 
          RowBox[{"MapIndexed", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"(", 
              RowBox[{"Flatten", "[", 
               RowBox[{"{", 
                RowBox[{
                 RowBox[{"pcolr", "[", 
                  RowBox[{
                   RowBox[{"First", "[", "#2", "]"}], "/", 
                   RowBox[{"Length", "[", "x", "]"}]}], "]"}], ",", 
                 RowBox[{"Point", "[", "#1", "]"}]}], "}"}], "]"}], ")"}], 
             "&"}], ",", "x"}], "]"}]}], ";"}], ")"}]}], "]"}], ";", "\n", 
     "\t\t", 
     RowBox[{"Show", "[", 
      RowBox[{
       RowBox[{"Graphics", "[", "ptdots", "]"}], ",", 
       RowBox[{
        RowBox[{
         RowBox[{"FilterOptions", "[", 
          RowBox[{"Graphics", ",", " ", "##"}], "]"}], "&"}], " ", "@@", "\n",
         "\t\t\t      ", 
        RowBox[{"Flatten", "[", 
         RowBox[{"{", 
          RowBox[{"opts", ",", " ", 
           RowBox[{"Options", "[", "colorListPlot", "]"}]}], "}"}], "]"}]}]}],
       "]"}]}]}], "\n", "\t\t", "]"}]}]], "Input",
 InitializationCell->True],

Cell[TextData[{
 "In order that our function behaves in a way analogous to ",
 StyleBox["ListPlot", "Input"],
