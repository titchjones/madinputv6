}], "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"colorListPlot", "[", 
   RowBox[{
    RowBox[{"x_", "/;", 
     RowBox[{
      RowBox[{"Length", "[", 
       RowBox[{"Dimensions", "[", "x", "]"}], "]"}], "==", "1"}]}], ",", 
    "opts___"}], "]"}], ":=", 
  RowBox[{"colorListPlot", "[", 
   RowBox[{
    RowBox[{"MapIndexed", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"First", "[", "#2", "]"}], ",", "#1"}], "}"}], "&"}], ",", 
      "x"}], "]"}], ",", "opts"}], "]"}]}]], "Input",
 InitializationCell->True],

Cell[TextData[{
 "In response to a common need (see mail from Sean Ross), here is a further \
overloading  to take ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{"x", ",", "y", ",", "z"}], ")"}], TraditionalForm]]],
 " values"
}], "Text"],

Cell[BoxData[
 RowBox[{
  RowBox[{"colorListPlot", "[", 
   RowBox[{
    RowBox[{"data3", ":", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"_", ",", " ", "_", ",", "_"}], "}"}], ".."}], "}"}]}], ",", 
    "opts___Rule"}], "]"}], ":=", "\n", "\t", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "data2", ",", "dataz", ",", "zhuecolor", ",", "ldata", ",", "a", ",", 
      "b"}], "}"}], ",", "\n", "\t\t", 
    RowBox[{
     RowBox[{"ldata", "=", 
      RowBox[{"Length", "[", "data3", "]"}]}], ";", "\n", "\t\t", 
     RowBox[{"data2", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"Take", "[", 
         RowBox[{"#", ",", "2"}], "]"}], "&"}], " ", "/@", "data3"}]}], ";", 
     "\n", "\t\t", 
     RowBox[{"dataz", "=", 
      RowBox[{"N", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Last", "[", "#", "]"}], "&"}], " ", "/@", " ", "data3"}], 
       "]"}]}], ";", "\n", "\t\t", 
     RowBox[{"a", "=", 
      RowBox[{"Min", "[", "dataz", "]"}]}], ";", 
     RowBox[{"b", "=", 
      RowBox[{"Max", "[", "dataz", "]"}]}], ";", "\n", "\t\t", 
     RowBox[{"dataz", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"dataz", "-", "a"}], ")"}], "/", 
       RowBox[{"(", 
        RowBox[{"b", "-", "a"}], ")"}]}]}], ";", "\n", "\t\t", 
     RowBox[{
      RowBox[{"zhuecolor", "[", "z_", "]"}], ":=", 
      RowBox[{"Hue", "[", 
       RowBox[{"0.8", " ", 
        RowBox[{"dataz", "[", 
         RowBox[{"[", 
          RowBox[{"Floor", "[", 
           RowBox[{"1", "+", 
            RowBox[{"z", 
             RowBox[{"(", " ", 
              RowBox[{"ldata", "-", "1"}], ")"}]}]}], "]"}], "]"}], "]"}]}], 
       " ", "]"}]}], ";", "\n", "\t\t", 
     RowBox[{"colorListPlot", "[", 
      RowBox[{"data2", ",", 
       RowBox[{"ColorScale", "->", "zhuecolor"}], ",", "opts"}], "]"}]}]}], 
   "\n", "\t\t", "]"}]}]], "Input",
 InitializationCell->True]
